{
    "project_name": "newproject",
    "projects_base": "projects",
    "data_type": "rnaseq",
    "rnaseq_samples_dir": "",
    "rnaseq_samples": "",
    "fastq_suffix": "fastq.gz",
    "paired_end_read": "yes",
    "read1_identifier": "_1",
    "read2_identifier": "{{ cookiecutter.read1_identifier|replace('1', '2') }}",
